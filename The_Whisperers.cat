            <characteristic name="Faction" typeId="7f76-cafe-10e5-f259">Whisperers</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <rules>
        <rule id="be38-7920-a2ef-26ab" name="Leader Ability: Unchallenged" hidden="false">
          <description>For the purposes of the Whisperer and One of the Herd rules, Alpha treats all friendly Whisperer models as Walkers.</description>
        </rule>
        <rule id="169f-0a37-1b62-03bc" name="Animals Need a Leader" hidden="false">
          <description>If Alpha is included in the group, she must be the Leader.</description>
        </rule>
        <rule id="d9fd-e37c-91a7-8018" name="The Alpha" hidden="false">
          <description>When using the Whisperer rule, Alpha may move all Walkers in her Kill Zone as a single action, instead of just one.</description>
        </rule>
      </rules>
